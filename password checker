using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
//Q8.Accept a 2 string from user and check if both are same or not . 
//    if it is different the print message “password and reenter password has to be same” 
//    and if it same print message “password updated successfully”. Give use 2 more chance
//    if password and reenter password
//Is not same.

namespace password_chech
{
    internal class Program
    {
        static void Main(string[] args)
        {
            for (int i = 0; i < 2; i++)
            {


                Console.WriteLine("Enter String 1");
                string s1 = Console.ReadLine();
                Console.WriteLine("Enter String 2");
                string s2 = Console.ReadLine();
                if (s1.Equals(s2))
                {
                    Console.WriteLine("Password updated sucessfully");
                }
                else
                {
                    Console.WriteLine("password_chech and reenter password has to be same");
                }
            }
        }
    }
}
